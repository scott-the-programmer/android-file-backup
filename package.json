{
  "name": "droid-up",
  "description": "A simple Android backup utility",
  "version": "1.0.0",
  "author": "Scott Murray",
  "bin": {
    "droid-up": "./bin/run"
  },
  "bugs": "https://github.com/scott-the-programmer/droid-up/issues",
  "dependencies": {
    "@oclif/command": "^1.8.19",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-help": "^3.2.10",
    "@types/get-folder-size": "^2.0.0",
    "@types/glob": "^7.1.1",
    "@types/ncp": "^2.0.3",
    "cli-ux": "^5.4.5",
    "fs": "0.0.2",
    "get-folder-size": "^2.0.1",
    "glob": "^7.2.2",
    "ncp": "^2.0.0",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "sinon": "^9.0.2",
    "ts-jest": "^27.0.3",
    "ts-mock-imports": "^1.3.0",
    "jest": "^27.0.5",
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@types/chai": "^4.2.11",
    "@types/node": "^10.17.20",
    "@types/jest": "^27.0.3",
    "chai": "^4.2.0",
    "globby": "^10.0.2",
    "nyc": "^14.1.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/scott-the-programmer/droid-up",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "droid-up",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "scott-the-programmer/droid-up",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}